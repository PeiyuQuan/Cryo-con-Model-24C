record (mbbi, $(P)$(M)getloopsource)
	{
		field(DTYP, "stream")
		field(INP, "@cryocon_24c_loop.proto getloopsource($(N)) $(PORT)")
		field(SCAN, "1 second")
		field(ZRVL, 0)
		field(ONVL, 1)
		field(TWVL, 2)
		field(THVL, 3)
		field(ZRST, "CHA")
		field(ONST, "CHB")
		field(TWST, "CHC")
		field(THST, "CHD")
	}
	
